#include<stdio.h>
 int i=0;
int while_loop(int i)
{
    while(i!=10)
    {
        printf("%d ",i);
        i++;
    }
}
int do_while_loop(int i)
{
    do
    {
        printf("%d ",i);
        i++;
    }
    while(i!=10);
}
int for_loop(int i)
{
    for(;i<10;i++)
    {
        printf("%d ",i);
    }
}
int main()
{
   // while_loop(i);
   // do_while_loop(i);
    for_loop(i);
}
