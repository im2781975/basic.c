
/*19.987654
19.99
19.988
1.998765e+01
19.987654
6.250000
0x7ffefa58edac 0x401240  */
#include <stdio.h>
int main() { 
  float num = 19.987654321;
  printf("%f\n", num); 
  printf("%.2f\n",num);
  printf("%4.3f\n",num);
  printf("%e\n", num); 
  printf("%lf\n",num);
  //typecasting
  int a,*p;
  double b,*q;
  b=(double)25/4;
  p=&a;
  q=(double*)q;
  printf("%lf\n",b);
  printf("%p %p",p,q);
  
  
}
